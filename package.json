{
  "name": "irasutoya",
  "version": "1.0.2",
  "description": "irasutoya (www.irasutoya.com) client",
  "main": "server.js",
  "bin": {
    "irasutoya-server": "./bin/irasutoya-server"
  },
  "scripts": {
    "start": "node bin/irasutoya-server",
    "prepare": "npm run build",
    "build": "npm run lint && npm-run-all -p build:*",
    "build:src": "tsc --project .",
    "build:test": "tsc --project test/",
    "watch": "tsc --watch",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "test": "npm run build:test && mocha out/**/*.js"
  },
  "engines": {
    "node": "9.x"
  },
  "keywords": [
    "irasutoya",
    "いらすとや"
  ],
  "author": "tattn (Tatsuya Tanaka)",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/express": "^4.11.0",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.2",
    "@types/node-fetch": "^1.6.7",
    "@types/power-assert": "^1.4.29",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "power-assert": "^1.4.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.2",
    "node-fetch": "^2.0.0-alpha.9",
    "request": "^2.83.0",
    "source-map-support": "^0.5.0"
  }
}
